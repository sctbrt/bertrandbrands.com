{
  "buildCommand": null,
  "outputDirectory": "src",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/pricing/access",
      "destination": "/api/pricing/access"
    },
    {
      "source": "/sudbury-brand-website-clarity",
      "destination": "/pages/sudbury-brand-website-clarity.html"
    },
    {
      "source": "/sudbury-small-business-website-leads",
      "destination": "/pages/sudbury-small-business-website-leads.html"
    },
    {
      "source": "/website-snapshot-review",
      "destination": "/pages/ads/website-snapshot-review.html"
    },
    {
      "source": "/brand-clarity-diagnostic",
      "destination": "/pages/ads/brand-clarity-diagnostic.html"
    },
    {
      "source": "/intake/website-snapshot-review",
      "destination": "/pages/intake/website-snapshot-review.html"
    },
    {
      "source": "/intake/brand-clarity-diagnostic",
      "destination": "/pages/intake/brand-clarity-diagnostic.html"
    }
  ],
  "redirects": [
    {
      "source": "/about",
      "destination": "/#about",
      "permanent": false
    },
    {
      "source": "/services",
      "destination": "/#services",
      "permanent": false
    },
    {
      "source": "/process",
      "destination": "/#process",
      "permanent": false
    },
    {
      "source": "/how-it-works",
      "destination": "/#process",
      "permanent": false
    },
    {
      "source": "/contact",
      "destination": "/#intake",
      "permanent": false
    },
    {
      "source": "/intake",
      "destination": "/#intake",
      "permanent": false
    },
    {
      "source": "/start",
      "destination": "/#intake",
      "permanent": false
    },
    {
      "source": "/start-here",
      "destination": "/#intake",
      "permanent": false
    },
    {
      "source": "/portal",
      "destination": "/#portal",
      "permanent": false
    },
    {
      "source": "/client-portal",
      "destination": "/#portal",
      "permanent": false
    },
    {
      "source": "/founders-direction-check",
      "destination": "/#services",
      "permanent": false
    },
    {
      "source": "/brand-website-starter-map",
      "destination": "/#services",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://www.googletagmanager.com https://www.google-analytics.com https://formspree.io; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https://www.google-analytics.com https://www.googletagmanager.com; connect-src 'self' https://formspree.io https://api.pushover.net https://ip-api.com https://www.google-analytics.com https://region1.google-analytics.com; frame-ancestors 'self'; form-action 'self' https://formspree.io; base-uri 'self'"
        }
      ]
    }
  ]
}
